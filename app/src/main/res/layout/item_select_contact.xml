<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_5sdp"
    android:layout_marginBottom="@dimen/size_10"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="@dimen/_42sdp"
        android:layout_height="@dimen/_42sdp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/blue_circle">

        <RelativeLayout
            android:layout_width="@dimen/_42sdp"
            android:layout_height="@dimen/_42sdp">

            <com.askonlinesolutions.wengage.utils.CircleImageView
                android:id="@+id/item_my_contacts_image"
                android:layout_width="@dimen/_40sdp"
                android:layout_height="@dimen/_40sdp"
                android:layout_centerInParent="true"
                android:layout_margin="@dimen/_5sdp"
                android:src="@drawable/profileg" />


        </RelativeLayout>

        <com.askonlinesolutions.wengage.utils.CircleImageView
            android:id="@+id/delete"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="true"

            android:src="@drawable/cancel" />

    </RelativeLayout>


    <com.askonlinesolutions.wengage.CustomTexts.CustomTextViewRegular
        android:id="@+id/item_my_contacts_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_toRightOf="@+id/item_my_contacts_image"
        android:text="Kelly"
        android:textColor="@color/text_color"
        android:textSize="@dimen/size_16" />

</LinearLayout>